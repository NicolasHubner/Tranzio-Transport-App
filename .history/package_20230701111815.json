{
  "private": true,
  "name": "my-app",
  "version": "1.0.3",
  "scripts": {
    "dev": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "start": "expo start --dev-client",
    "prebuild:debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:debug": "cd android && gradlew clean && gradlew assembleDebug",
    "postbuild:debug": "rm -rf ./android/app/src/main/res/drawable-* && echo \"O arquivo está em ./android/app/build/outputs/apk/debug\"",
    "prebuild:release": "rm -rf ./android/app/src/main/res/drawable-*",
    "build:release": "cd android && gradlew clean && gradlew assembleRelease --stacktrace",
    "postbuild:release": "echo \"O arquivo está em ./android/app/build/outputs/apk/release\"",
    "eas-build:remote": "eas build -p android --profile preview",
    "eas-build:local": "eas build -p android --profile preview --local"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-native-fontawesome": "^0.3.0",
    "@hookform/resolvers": "^3.1.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "9.3.7",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "appcenter": "4.4.5",
    "appcenter-analytics": "4.4.5",
    "appcenter-crashes": "4.4.5",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
    "expo": "~48.0.9",
    "expo-barcode-scanner": "~12.3.2",
    "expo-device": "~5.2.1",
    "expo-image-picker": "~14.1.1",
    "expo-location": "~15.1.1",
    "expo-notifications": "~0.18.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "intl": "^1.2.5",
    "nativewind": "^2.0.11",
    "phosphor-react": "^1.4.1",
    "react": "18.2.0",
    "react-hook-form": "^7.45.0",
    "react-native": "0.71.8",
    "react-native-autocomplete-input": "^5.3.2",
    "react-native-dropdown-autocomplete": "^1.0.18",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.8.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-steps": "^1.3.0",
    "react-native-svg": "13.4.0",
    "react-native-switch-toggle": "^2.2.1",
    "react-native-toast-message": "^2.1.6",
    "react-native-vector-icons": "^9.2.0",
    "react-select": "^5.7.3",
    "react-toastify": "^9.1.3",
    "styled-components": "^5.3.11",
    "tailwind-react-native-classnames": "^1.5.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eas-cli": "^3.14.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.4"
  }
}
